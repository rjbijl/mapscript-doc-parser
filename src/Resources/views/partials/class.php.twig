{% if class.descriptions is not empty %}
 /**
{% for line in class.descriptions %}
  * {{ line|raw }}
{% endfor %}
  */
{% endif %}
final class {{ class.className }}
{
{% for member in class.members %}
    /**
{% if member.description is not empty %}
{% for line in member.description %}
     * {{ line|raw }}
{% endfor %}
     *
{% endif %}
     * @var {{ member.type }}
     */
    public ${{ member.name }};

{% endfor %}
{% for method in class.constructors %}
{% include 'partials/method.php.twig' with {'method': method} %}
{% endfor %}
{% for method in class.methods %}
{% include 'partials/method.php.twig' with {'method': method} %}
{% endfor %}
}
